{
  "Lambda": {
    "Handler": "DetectCelebrities.lambda_handler",
    "MemorySize": 256,
    "TimeoutSecs": 20,
    "IAMPolicyDocument": [
      {
        "Actions": [
          "rekognition:*"
        ],
        "Resources": [
          "*"
        ]
      }
    ],
    "Layers": [
      "opencv-python"
    ]
  },
  "MRE": {
    "Plugin": {
      "Name": "DetectCelebrities",
      "Description": "A plugin to detect celebrities using the Amazon Rekognition API",
      "Class": "Featurer",
      "ExecutionType": "Sync",
      "SupportedMediaType": "Video",
      "ContentGroups": [
        "All"
      ],
      "ExecuteLambdaQualifiedARN": "",
      "Configuration": {
        "minimum_confidence": 30,
        "celebrity_list": "['Joe Biden','Donald Trump','Tom Cruise','Kamala Harris','Kevin McCarthy']"
      },
      "OutputAttributes": {
        "Label": {
          "Description": "",
          "flag_celebrity1": "",
          "flag_celebrity2": "",
          "flag_celebrity3": "",
          "flag_celebrity4": "",
          "flag_celebrity5": ""
        }
      }
    }
  }
}