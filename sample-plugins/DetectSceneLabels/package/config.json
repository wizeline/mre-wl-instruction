{
  "Lambda": {
    "Handler": "DetectSceneLabels.lambda_handler",
    "MemorySize": 256,
    "TimeoutSecs": 180,
    "IAMPolicyDocument": [
      {
        "Actions": [
          "sagemaker:*"
        ],
        "Resources": [
          "*"
        ]
      }
    ],
    "Layers": [
      "opencv_python"
    ]
  },
  "MRE": {
    "Plugin": {
      "Name": "DetectSceneLabels",
      "Description": "A plugin to detect scene labels using GenAI",
      "Class": "Featurer",
      "ExecutionType": "Sync",
      "SupportedMediaType": "Video",
      "ContentGroups": [
        "All", "News"
      ],
      "ExecuteLambdaQualifiedARN": "",
      "Configuration": {     
        "minimum_confidence": 30
      },
      "OutputAttributes": {
        "Label": {
          "Description": ""
        }
      }
    }
  }
}
